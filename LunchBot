function main() {
  var formId = '----------------------------------------------';    // formId
  var tokens = ['----------------------------------------------'];   // Line Notify token กรณีส่งเข้าไลน์มากกว่าสองกลุ่ม ให้คั่นแต่ละ Token ด้วย , 
  LunchNotifyBot.LunchBot (formId, tokens);
}

